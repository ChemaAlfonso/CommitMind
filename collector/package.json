{
	"name": "lean-dev-metrics-collector",
	"version": "1.0.0",
	"description": "Collector service for developer productivity metrics",
	"main": "dist/index.js",
	"author": "Chema Alonso <hola@chemaalfonso.com>",
	"scripts": {
		"build": "tsc",
		"start": "node index.js",
		"dev": "tsx watch src/index.ts",
		"migrate": "node db/migrations.js",
		"seed": "tsx src/scripts/seed-historical-data.ts",
		"seed:prod": "node scripts/seed-historical-data.js",
		"summary": "tsx src/scripts/get-summary.ts",
		"summary:prod": "node scripts/get-summary.js",
		"report": "tsx src/scripts/send-report.ts",
		"report:prod": "node scripts/send-report.js"
	},
	"dependencies": {
		"express": "^4.18.2",
		"sqlite3": "^5.1.6",
		"node-cron": "^3.0.3",
		"axios": "^1.6.2",
		"@octokit/rest": "^20.0.2",
		"@gitbeaker/rest": "^39.34.3",
		"winston": "^3.11.0",
		"dotenv": "^16.3.1",
		"cors": "^2.8.5",
		"helmet": "^7.1.0",
		"openai": "^4.20.1",
		"@anthropic-ai/sdk": "^0.24.3"
	},
	"devDependencies": {
		"@types/express": "^4.17.21",
		"@types/node": "^20.10.5",
		"@types/node-cron": "^3.0.11",
		"@types/cors": "^2.8.17",
		"typescript": "^5.3.3",
		"tsx": "^4.7.0"
	}
}
